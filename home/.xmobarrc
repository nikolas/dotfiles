Config {
    font = "-*-neep-medium-*-*-*-13-*-*-*-*-*-*-*",
    commands = [
        Run Weather "KNYC" [
            "-t","<station>: <tempF>F",
            "-L", "18",
            "-H", "25",
            "--normal", "green",
            "--high", "red",
            "--low", "lightblue" ] 36000,
        Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
    ],
    sepChar = "%",
    alignSep = "}{",
    template = "}{ %KNYC% | <fc=#ee9a00>%date%</fc>"
}
